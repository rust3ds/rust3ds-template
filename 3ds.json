{
  "data-layout": "e-m:e-p:32:32-i64:64-v128:64:128-a:0:32-n32-S64", 
  "llvm-target": "arm-none-eabihf",
  "linker": "arm-none-eabi-gcc",
  "ar": "arm-none-eabi-ar",
  "target-endian": "little",
  "target-pointer-width": "32",
  "arch": "arm",
  "os": "linux",
  "cpu": "mpcore",
  "features": "+vfp2",
  "relocation-model": "static",
  "executables": true,
  "exe-suffix": ".elf",
  "pre-link-args": [
    "-specs=3dsx.specs",
    "-march=armv6k",
    "-mtune=mpcore",
    "-mfloat-abi=hard",
    "-mtp=soft"
  ],
  "post-link-args": [
    "-lc",
    "-lm",
    "-lsysbase",
    "-lc",
    "-lsysbase",
    "-lc"
  ]
}
